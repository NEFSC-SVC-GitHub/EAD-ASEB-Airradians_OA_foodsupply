ClearRate.table$Run           <- gsub("^(?:[^_]+_){3}([^_]+).*", "\\1", ClearRate.table$ID)
ClearRate.table$Time_period   <- paste((as.numeric(substr(dat2$Time._min,1,1)) -1), "0-", dat2$Time._min, sep ='')
